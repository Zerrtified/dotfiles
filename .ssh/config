Host github.com
    # github
    HostName github.com
    # Username for remote SSH User
    User git
    # Path to Private Key
    IdentityFile ~/.ssh/_github_rsa

Host bitbucket.org
    # Bitbucket
    HostName bitbucket.org
    # Username for remote SSH User
    User git
    # Path to Private Key
    IdentityFile ~/.ssh/_bitbucket_rsa
